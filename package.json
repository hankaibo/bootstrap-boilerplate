{
  "name": "mybootstrap",
  "version": "3.0.0",
  "description": "An open source, admin dashboard template for Bootstrap.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf node_modules dist && npm cache clean",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean-install && npm start",
    "watch": "webpack --watch --progress --profile",
    "build": "rimraf dist && webpack --progress --profile --bail",
    "server": "webpack-dev-server --inline --hot --port 8080",
    "start": "npm run server"
  },
  "repository": {
    "type": "git",
    "url": "https://git.oschina.net/hankaibo/mybootstrap.git"
  },
  "keywords": [
    "webpack bootstrap"
  ],
  "author": "hankaibo",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "bootstrap-social": "^5.0.0",
    "datatables": "^1.10.12",
    "datatables-bootstrap": "0.0.1",
    "datatables-responsive": "^1.0.7",
    "font-awesome": "^4.6.3",
    "holderjs": "^2.9.4",
    "jquery": "^3.1.0",
    "metismenu": "^2.5.2"
  },
  "devDependencies": {
    "css-loader": "^0.25.0",
    "eslint": "^3.3.1",
    "eslint-config-google": "^0.6.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "glob": "^7.0.6",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "node-sass": "^3.10.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1"
  }
}
