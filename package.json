{
  "name": "mybootstrap",
  "version": "2.0.0",
  "description": "An open source, admin dashboard template for Bootstrap.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf node_modules dist && npm cache clean",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean-install && npm start",
    "watch": "webpack --watch --progress --profile",
    "build": "rimraf dist && webpack --progress --profile --bail",
    "server": "webpack-dashboard -- webpack-dev-server --inline --port 8080",
    "webdriver-update": "webdriver-manager update",
    "webdriver-start": "webdriver-manager start",
    "start": "npm run server",
    "postinstall": "npm run webdriver-update"
  },
  "repository": {
    "type": "git",
    "url": "https://git.oschina.net/hankaibo/mybootstrap.git"
  },
  "keywords": [
    "bootstrap"
  ],
  "author": "hankaibo",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "bootstrap-social": "^5.0.0",
    "datatables": "^1.10.12",
    "datatables-responsive": "^1.0.7",
    "echarts": "^3.2.3",
    "font-awesome": "^4.6.3",
    "holderjs": "^2.9.3",
    "jquery": "^3.1.0",
    "metismenu": "^2.5.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.2",
    "codelyzer": "0.0.26",
    "copy-webpack-plugin": "^3.0.0",
    "css-loader": "^0.23.0",
    "eslint": "^3.3.1",
    "eslint-config-google": "^0.6.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "node-sass": "^3.4.2",
    "null-loader": "0.1.1",
    "postcss-loader": "^0.9.1",
    "rimraf": "^2.5.1",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.13",
    "webpack-dashboard": "^0.1.8",
    "webpack-dev-server": "^1.14.1"
  }
}
