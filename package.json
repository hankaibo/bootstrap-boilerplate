{
  "name": "mybootstrap",
  "version": "3.3.7",
  "description": "An open source, admin dashboard template for Bootstrap.",
  "scripts": {
    "clean": "rimraf node_modules dist && npm cache clean",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean-install && npm start",
    "watch": "webpack --watch --progress --profile",
    "build": "rimraf dist && webpack --progress --profile --bail",
    "server": "webpack-dev-server --inline --hot --port 8080",
    "start": "npm run server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hankaibo/mybootstrap.git"
  },
  "keywords": [
    "webpack bootstrap"
  ],
  "author": "hankaibo",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "bootstrap-social": "^5.0.0",
    "d3": "^4.3.0",
    "d3-tip": "^0.7.1",
    "datatables": "^1.10.12",
    "datatables-bootstrap": "0.0.1",
    "datatables-responsive": "^1.0.7",
    "font-awesome": "^4.6.3",
    "jquery": "^3.1.0",
    "metismenu": "^2.5.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.1",
    "css-loader": "^0.26.1",
    "copy-webpack-plugin": "^4.0.1",
    "eslint": "^3.3.1",
    "eslint-config-google": "^0.6.0",
    "extract-text-webpack-plugin": "~2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "glob": "^7.0.6",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.28.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "node-sass": "^4.5.0",
    "postcss-loader": "^0.13.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.0",
    "sass-loader": "^6.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.3.0"
  }
}
